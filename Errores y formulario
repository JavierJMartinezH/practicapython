numHuevos= 12

#Opcion 1
print("Tengo "+ str(numHuevos)+ " huevos.")

#Opcion 2
print("Tengo %s huevos." %(numHuevos))  #%s para decir que va una variable      %variable

#Calcular la superficie o Ã¡rea de un cuadrado

lado=int(input("Ingrese la medida del lado del cuadrado: "))
superficie= lado * lado
print("La suoperficie del cuadrado es: " + str(superficie))

